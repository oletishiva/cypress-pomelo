{
  "projectId": "t1k2ob",
  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "configFile": "cypress/support/cypress-multi-reporters.json"
  },
  "video": false,
  "ignoreTestFiles": [
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ],
  "env": {
    "cypress-plugin-snapshots": {
      "imageConfig": {
        "threshold": 0.01
      }
    },
    "allureResultsPath": "allure-results",
    "allure": true
  },
  "baseUrl": "https://www.pomelofashion.com/th/en/",
  "defaultCommandTimeout": 30000,
  "retries": 0
}
